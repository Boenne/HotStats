<UserControl x:Class="HotStats.UserControls.MatchDetailsUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HotStats.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             Height="300" Width="300"
             DataContext="{Binding MatchDetailsViewModel, Source={StaticResource ViewModelLocator}}">
    <Border>
            <StackPanel>
                <Label Content="Match details" FontSize="20" HorizontalAlignment="Center"></Label>
                
            </StackPanel>
    </Border>
</UserControl>
